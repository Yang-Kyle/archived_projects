<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
    <!--    【学生注册/登录】 -->
    <servlet>
        <servlet-name>StuSignUp</servlet-name>
        <servlet-class>Servlets.StuSignUp</servlet-class>
        <!--   优化加载速度     <load-on-startup>30</load-on-startup>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>StuSignUp</servlet-name>
        <url-pattern>/S00</url-pattern>
    </servlet-mapping>
    <!--    【学生查询】-->
    <servlet>
        <servlet-name>StuQ</servlet-name>
        <servlet-class>Servlets.StuQ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StuQ</servlet-name>
        <url-pattern>/S01</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BusSignUp</servlet-name>
        <servlet-class>Servlets.StuSignUp</servlet-class>
        <!--   优化加载速度     <load-on-startup>30</load-on-startup>-->
    </servlet>
    <servlet-mapping>
        <servlet-name>BusSignUp</servlet-name>
        <url-pattern>/B00</url-pattern>
    </servlet-mapping>
